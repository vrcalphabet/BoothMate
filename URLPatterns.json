{
  "urls": {
    "html_urls": {
      "search": [
        "https://booth.pm/{lang}/browse/{encoded_string}{parameters}",
        "https://booth.pm/{lang}/search/{encoded_string}{parameters}",
        "https://booth.pm/{lang}/events/{encoded_string}{parameters}",
        "https://booth.pm/{lang}/items{parameters}"
      ],
      "item": [
        "https://booth.pm/{lang}/items/{item_id}{parameters}",
        "https://{subdomain}.booth.pm/items/{item_id}{parameters}"
      ],
      "shop": [
        "https://{subdomain}.booth.pm/{parameters}"
      ],
      "shop_items": [
        "https://{subdomain}.booth.pm/items{parameters}"
      ],
      "shop_list_items": [
        "https://{subdomain}.booth.pm/item_lists/{item_list_id}"
      ],
      "wishlist_items": [
        "https://accounts.booth.pm/wish_lists/{wishlist_id}{parameters}",
        "https://booth.pm/wish_list_names/{wishlist_id}{parameters}"
      ]
    },
    "json_urls": {
      "wishlist_counts": [
        "https://accounts.booth.pm/wish_lists.json{parameters}"
      ],
      "item": [
        "https://booth.pm/{lang}/items/{item_id}.json"
      ],
      "manifest": [
        "https://booth.pm/manifest.json"
      ],
      "autocomplete": [
        "https://booth.pm/autocomplete/tag.json{parameters}"
      ],
      "wishlist_names": [
        "https://accounts.booth.pm/wish_list_names.json"
      ],
      "wishlist_items": [
        "https://api.booth.pm/frontend/wish_list_names/{wishlist_id}/items.json{parameters}",
        "https://accounts.booth.pm/wish_list_name_items.json{parameters}"
      ],
      "wishlist_name": [
        "https://api.booth.pm/frontend/wish_list_names/{wishlist_id}.json",
        "https://api.booth.pm/frontend/accounts/wish_list_names/{wishlist_id}.json"
      ],
      "item_in_wishlist": [
        "https://booth.pm/items/{item_id}/wish_list_names.json"
      ],
      "wishlist_post_patch_delete": [
        "https://booth.pm/items/{item_id}/wish_list"
      ]
    }
  },
  "variables": {
    "lang": "^[a-zA-Z-]+$",
    "encoded_string": "^(%[A-F0-9]{2})+$",
    "parameters": "^([\\?&#][\\w\\[\\]]+(=[^&]*)?)*$",
    "item_id": "^\\d+$",
    "subdomain": "^[a-z0-9-]{1,16}$",
    "wishlist_id": "^[a-zA-Z0-9]{8}$",
    "shop_list_items": "^[a-zA-Z0-9]{8}$"
  }
}
